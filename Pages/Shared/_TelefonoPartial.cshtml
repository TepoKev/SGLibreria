@model Proveedores.ListaProveedorModel
@foreach(var tel in Model.Telefonos){
<tr>
    <td>
        <input type="text" class="form-control" value="@tel.Numero" data-n="@tel.Numero" id="t @tel.Id" disabled>
    </td>
    @if(tel.Principal == 1){
    <td>
        <div class="custom-control custom-switch text-center">
            <input type="checkbox" class="custom-control-input" id="i @tel.Id" data-number="@tel.Numero"
                data-idprov="@tel.IdProveedor" checked disabled>
            <label class="custom-control-label" for="i @tel.Id"></label>
        </div>
    </td>
    }else{
    <td>
        <div class="custom-control custom-switch text-center">
            <input type="checkbox" class="custom-control-input" id="i @tel.Id" data-number="@tel.Numero" data-idprov="@tel.IdProveedor">
            <label class="custom-control-label" for="i @tel.Id"></label>
        </div>
    </td>
    }
    <td>
        <div class="btn-group">
            <button type="button" class="btn btn-outline-warning">
                <i class="fas fa-pencil-alt" data-tid="t @tel.Id"></i>
            </button>
            <button type="button" class="btn btn-outline-danger">
                <i class="fas fa-trash" data-elim="@tel.Id"></i>
            </button>
        </div>
    </td>
</tr>
}